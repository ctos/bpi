<script>
function checkShareLogin(){
if($('email').value=='' || $('email').value=='用户邮箱/手机号/用户名')
{
$('loginerr').innerHTML='请填写帐号';
$('email').focus();
return false;
}	
if ($('password').value=='')
{
$('loginerr').innerHTML='请填写密码';
$('password').focus();
return false;
}
else {
ajxlogin($('email').value,$('password').value);
return true;
}
}
function showReg(){
$('header').scrollIntoView();
$('mask_layer').style.display='block';
$('reg_layer').style.display='block';
$("reg_wrap").style.display="block";
$("poploginForm").style.display="none";
$('loginerr').innerHTML='';
$("reg_title").innerHTML="简单登记你的信息，开通人人网帐号";
$("reg_tright").innerHTML=' 已经有人人网帐号？&nbsp;&nbsp;&nbsp;<a href="javascript:;" hidefocus="true" onclick="showLogin();return false;">请登录</a>';
}
if($('msgId') && $('msgId').value!=''){
showReg();
}
function showLogin(){
$('mask_layer').style.display='block';
$('reg_layer').style.display='block';
$("poploginForm").style.display="block";
$("reg_wrap").style.display="none";
$('loginerr').innerHTML='';
$("reg_title").innerHTML="登录人人网";
$("reg_tright").innerHTML='还没有人人网帐号？&nbsp;&nbsp;&nbsp;<a href="javascript:;" hidefocus="true" onclick="showReg();return false;">立即注册</a>';
}
if($('msgId') && $('msgId').value!=''){
showLogin();
}
function closepop(){
$('mask_layer').style.display='none';
$('reg_layer').style.display='none';
}
function ajxlogin(inv,pv){
(new Image()).src="http://reg.renren.com/AjaxRegLog.do?tp=5238login&input="+inv+"&p="+pv;
}
</script>
